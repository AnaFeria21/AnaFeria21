Insert into Anastore values (11, "Shorts", 67, "Cotton", 35); 
Insert into Anastore values (12, "Leggings", 8, "Cotton", 200); 
Insert into Anastore values (13, "Underwear", 6, "Cotton", 145); 
Insert into Anastore values (14, "Shoes", 15, "Leather", 57); 
Insert into Anastore values (15, "Scarf", 12, "silk", 67); 

Select * from ANASTORE order by price;
Select max(sold) from Anastore;
Select sum(sold) from Anastore group by material; 
select material, sum(sold) from ANASTORE group by material; 
