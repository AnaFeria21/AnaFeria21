Create Table ANASTORE (ID integer primary key, Product_Type TEXT, Price Integer, material Text, Sold Integer);

Insert into Anastore values (1, "Sweater", 10, "Cotton", 100); 
Insert into Anastore values (2, "T-shirt", 5, "Cotton", 200); 
Insert into Anastore values (3, "Trousers", 20, "Linen", 150); 
